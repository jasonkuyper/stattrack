// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Player {
  id             Int    @id
  firstName      String
  lastName       String
  teamId         String
  jersey         String
  pos            String
  heightFeet     String
  heightInches   String
  weightPounds   String
  dateOfBirthUTC String
  yearsPro       String
  collegeName    String
  country        String
}
